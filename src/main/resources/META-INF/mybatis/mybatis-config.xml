<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="true"/>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
		<setting name="callSettersOnNulls" value="true"/>
	</settings>

	<typeAliases>
		<typeAlias type="java.util.HashMap" alias="paramMap" />
		<typeAlias type="java.util.List" alias="paramList" />
		<typeAlias type="java.util.HashMap" alias="resultMap" />
		<typeAlias type="java.util.List" alias="resultList" />
		<typeAlias type="co.kr.wdt.blog.vo.BlogVo" alias="blogvo" />
		<typeAlias type="co.kr.wdt.post.vo.PostVo" alias="postvo" />
		<typeAlias type="co.kr.wdt.comment.vo.CommentVo" alias="commentvo" />
		<typeAlias type="co.kr.wdt.reply.vo.ReplyVo" alias="replyvo" />
		<typeAlias type="co.kr.wdt.message.vo.MessageVo" alias="messagevo" />
	</typeAliases>
</configuration>